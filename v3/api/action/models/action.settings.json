{
  "kind": "collectionType",
  "collectionName": "actions",
  "info": {
    "name": "action",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "shared-blocks.contact",
        "shared-blocks.heading",
        "shared-blocks.media",
        "shared-blocks.material",
        "shared-blocks.accordion",
        "shared-blocks.richtext",
        "shared-blocks.unterbrecher",
        "shared-blocks.teaser-large",
        "shared-blocks.teasers-small",
        "shared-blocks.media-gallery",
        "shared-blocks.oembed",
        "shared-blocks.newsletter",
        "shared-blocks.actions"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "start": {
      "type": "datetime",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "location_detail": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "coordinates": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "end": {
      "type": "datetime",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "intro": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "link": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "location": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-smo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "group": {
      "via": "actions",
      "model": "group"
    },
    "campaign": {
      "via": "actions",
      "model": "campaign"
    }
  }
}
