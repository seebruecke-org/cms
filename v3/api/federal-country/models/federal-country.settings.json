{
  "kind": "collectionType",
  "collectionName": "federal_countries",
  "info": {
    "name": "Federal Country",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "shared-blocks.contact",
        "shared-blocks.heading",
        "shared-blocks.media",
        "shared-blocks.stage-medium",
        "shared-blocks.material",
        "shared-blocks.accordion",
        "shared-blocks.richtext",
        "shared-blocks.stage-large",
        "shared-blocks.unterbrecher",
        "shared-blocks.teaser-large",
        "shared-blocks.teasers-small",
        "shared-blocks.media-gallery",
        "shared-blocks.oembed",
        "shared-blocks.newsletter",
        "shared-blocks.actions"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "country": {
      "via": "federal_countries",
      "model": "country"
    },
    "cities": {
      "via": "federal_country",
      "collection": "city"
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-smo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "demands": {
      "type": "component",
      "repeatable": false,
      "component": "demands.demands",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
