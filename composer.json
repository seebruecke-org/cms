{
  "repositories": [
    {
      "type": "composer",
      "url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/wordpress-plugin/"
    },

    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],

  "require": {
    "advanced-custom-fields/advanced-custom-fields-pro": "^5.9",
    "wpackagist-plugin/disable-comments": "^2.0.2",
    "wpackagist-plugin/regenerate-thumbnails": "^3.1.4",
    "wpackagist-plugin/clean-image-filenames": "^1.3.0",
    "wpackagist-plugin/wp-graphql": "^1.0",
    "pristas-peter/wp-graphql-gutenberg": "dev-develop",
    "valu/wp-graphql-polylang": "dev-master",
    "wp-graphql/wp-graphql-acf": "dev-develop"
  },

  "minimum-stability": "dev",

  "autoload": {
    "psr-4": {
      "Seebruecke\\": "themes/seebruecke/src/classes/"
    }
  },

  "extra": {
    "installer-paths": {
      "plugins/{$name}": ["type:wordpress-plugin"]
    }
  }
}
