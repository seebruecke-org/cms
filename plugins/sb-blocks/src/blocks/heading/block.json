{
	"apiVersion": 2,
	"name": "sb/heading",
	"category": "text",
	"attributes": {
		"textAlign": {
			"type": "string"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "h1 > span, h2 > span, h3 > span, h4 > span, h5 > span, h6 > span",
			"default": ""
		},
		"level": {
			"type": "number",
			"default": 2
		},
		"placeholder": {
			"type": "string"
    },
    "kicker": {
      "type": "string",
      "source": "html",
			"selector": "h1 > small, h2 > small, h3 > small, h4 > small, h5 > small, h6 > small",
			"default": ""
    }
	},
	"supports": {
		"align": [ "wide", "full" ],
		"anchor": true,
		"className": false,
		"color": {
			"link": true
		},
		"fontSize": true,
		"lineHeight": true
	},
	"editorStyle": "wp-block-heading-editor",
	"style": "wp-block-heading"
}
