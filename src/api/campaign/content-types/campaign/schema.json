{
  "kind": "collectionType",
  "collectionName": "campaigns",
  "info": {
    "singularName": "campaign",
    "pluralName": "campaigns",
    "displayName": "Campaign",
    "name": "campaign"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "shared-blocks.contact",
        "shared-blocks.heading",
        "shared-blocks.media",
        "shared-blocks.stage-medium",
        "shared-blocks.material",
        "shared-blocks.accordion",
        "shared-blocks.richtext",
        "shared-blocks.stage-large",
        "shared-blocks.unterbrecher",
        "shared-blocks.teaser-large",
        "shared-blocks.teasers-small",
        "shared-blocks.media-gallery",
        "shared-blocks.newsletter",
        "shared-blocks.oembed",
        "shared-blocks.actions"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-smo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "actions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::action.action",
      "mappedBy": "campaign"
    }
  }
}
